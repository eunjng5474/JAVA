package day0321;

import java.util.Scanner;

public class Test03_배열과반복문 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] scores = new int[5];
		for(int i=0; i<5; i++) {
			scores[i] = sc.nextInt();
		}
		
		for(int i=0; i<5; i++) {
			System.out.println(i+"번째 점수는 :"+scores[i]);
		}
	}

}
